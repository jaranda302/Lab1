public class GradeAvg {

	public static void main(String[] args) {
		
		//creates double variables
		double quizScore1, quizScore2, quizScore3, testScore1, testScore2, quizScoreAvg, testScoreAvg;

		//initializes variables
		quizScore1 = 86;
		quizScore2 = 92;
		quizScore3 = 95;
		testScore1 = 78; 
		testScore2 = 96;
		quizScoreAvg = (quizScore1 + quizScore2 + quizScore3)/3;
		testScoreAvg = (testScore1 + testScore2)/2;
		
		//prints out the variable names and values
		System.out.println("quizScoreAvg = " + quizScoreAvg + " and testScoreAvg = " + testScoreAvg);
		
	}

}
